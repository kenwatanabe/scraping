<p id="notice"><%= notice %></p>

<h1>Listing Amazon Api Data</h1>

<table>
  <thead>
    <tr>
      <th>Id type</th>
      <th>Item</th>
      <th>Main image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @amazon_api_data.each do |amazon_api_datum| %>
      <tr>
        <td><%= amazon_api_datum.id_type %></td>
        <td><%= amazon_api_datum.item_id %></td>
        <td><%= amazon_api_datum.main_image_url %></td>
        <td><%= link_to 'Show', amazon_api_datum %></td>
        <td><%= link_to 'Edit', edit_amazon_api_datum_path(amazon_api_datum) %></td>
        <td><%= link_to 'Destroy', amazon_api_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Amazon api datum', new_amazon_api_datum_path %>
